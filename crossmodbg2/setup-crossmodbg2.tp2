BACKUP ~crossmodbg2\backup~
AUTHOR ~Grim Squeaker, K'aeloree, and Berelinde www.gibberlings3.net~

/* backwards compatibility is a PITA, but necessary */
NO_IF_EVAL_BUG

VERSION ~vBETA_15_07222015~

README ~crossmodbg2/readme-crossmodbg2.html~


ALWAYS
	ACTION_IF GAME_IS ~bgee tutu bg1~ THEN BEGIN
		PRINT ~WARNING. THIS MOD IS FOR BG2 OR BGT. DO NOT INSTALL THIS MOD.~
	END
	ACTION_IF GAME_IS ~bg2ee eet~ THEN BEGIN
		PRINT ~WARNING. THIS MOD IS NOT TRAIFIED OR OR CONVERTED TO utf8. YOUR GAME MIGHT CRASH ON SOME BANTERS.~
	END
	ACTION_IF GAME_IS ~soa~ THEN BEGIN
		PRINT ~BGII:SoA detected...~
	END
	ACTION_IF GAME_IS ~tob~ THEN BEGIN
		PRINT ~BGII:ToB detected...~
	END
	ACTION_IF GAME_IS ~bgt~ THEN BEGIN
		PRINT ~BGT detected...~
	END
END


